{
  "DatabaseInformation": {
    "Scheme": "mongodb",
    "ServerAddress": "127.0.0.1",
    "ServerPort": 27017,
    "ConnectionTimeoutSeconds": 60,
    "UseTls": true,
    "DirectConnection": true,
    "ServerSelectionTimeoutSeconds": 2,
    "ApplicationName": "NbaWebApp",
    "ConnectionString": "mongodb://127.0.0.1:27017/nba/?directConnection=true&serverSelectionTimeoutMS=2000&appName=NbaWebApp"
  },
  "DatabaseTables": {
    "PlayersCollectionName": "players",
    "ShotChartCollectionName": "shot-charts",
    "BoxScoreCollectionName": "box-scores"
  },
  "DefaultSeason": "2024-25",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
